{% extends "WikiIndexBundle::layout.html.twig" %}

{% block title %}
Ajouter un article | {{ parent() }}
{% endblock %}

{% block body %}
<h3>Ajouter un article</h3>
{% include "::alert.html.twig" %}
<div id="create" class="well">
	<form method="post" {{ form_enctype(form) }}>
		{{ form_widget(form) }}
		<div class="form-group">
			<label>Ajouter un tag</label>
			<input class="form-control tag-add"/>
		</div>
		<div class="tags"></div>
		<input type="submit" class="btn btn-primary" />
	</form>
</div>
<h3>Pr√©visualisation</h3>
<div class="preview"></div>
{% endblock %}